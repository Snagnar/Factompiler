# Working sample: Comparison and logical operations
# Tests: comparison operators, logical operations

let sensor_a = input(0);
let sensor_b = input(1);
let threshold = input(2);

# Comparison operations
let a_greater = sensor_a > threshold;
let b_less = sensor_b < threshold;
let equal_check = sensor_a == sensor_b;

# Logical operations
let both_active = a_greater && b_less;
let either_active = a_greater || b_less;
let not_equal = !equal_check;

# Output results
let output1 = both_active | "signal-1";
let output2 = either_active | "signal-2";
let output3 = not_equal | "signal-3";
