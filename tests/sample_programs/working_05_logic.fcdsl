# Working sample: Comparison and logical operations
# Tests: comparison operators, logical operations

Signal sensor_a = input(0);
Signal sensor_b = input(1);
Signal threshold = input(2);

# Comparison operations
Signal a_greater = sensor_a > threshold;
Signal b_less = sensor_b < threshold;
Signal equal_check = sensor_a == sensor_b;

# Logical operations
Signal both_active = a_greater && b_less;
Signal either_active = a_greater || b_less;
Signal not_equal = !equal_check;

# Output results
Signal output1 = both_active | "signal-1";
Signal output2 = either_active | "signal-2";
Signal output3 = not_equal | "signal-3";
